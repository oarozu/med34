{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Docente</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Modalidad</th>
                <td>{{ entity.modalidad }}</td>
            </tr>
            <tr>
                <th>Vinculacion</th>
                <td>{{ entity.vinculacion }}</td>
            </tr>
            <tr>
                <th>Cargo</th>
                <td>{{ entity.cargo }}</td>
            </tr>
            <tr>
                <th>Resolucion</th>
                <td>{{ entity.resolucion }}</td>
            </tr>
            <tr>
                <th>Perfil</th>
                <td>{{ entity.perfil }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('docente') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('docente_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
